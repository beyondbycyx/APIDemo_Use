<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hq.apidemo" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SpinnerActivity"
            android:label="@string/title_activity_spinner" >
        </activity>
        <activity
            android:name=".ViewFlipperActivity"
            android:label="@string/title_activity_view_flipper" >
        </activity>
        <activity
            android:name=".CompleteViewActivity"
            android:label="@string/title_activity_complete_view" >
        </activity>
        <activity
            android:name=".ChronometerDemo"
            android:label="@string/title_activity_chronometer_demo" >
        </activity>
    </application>

</manifest>
